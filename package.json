{
  "name": "VueExpress",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-server": "node ./bin/www",
    "watch-server": "nodemon ./bin/www",
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run dev -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv":"^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.1.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.1",
    "jquery": "^3.4.1",
    "laravel-mix": "^4.1.2",
    "lodash": "^4.17.14",
    "popper.js": "^1.12",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.22.4",
    "sass-loader": "^7.1.0",
    "sweetalert2": "^8.13.5",
    "vform": "^1.0.1",
    "vue": "^2.5.17",
    "vue-i18n": "^8.12.0",
    "vue-template-compiler": "^2.6.10",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  }
}
