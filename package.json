{
  "name": "ihub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start-server": "node ./bin/www",
    "watch-server": "nodemon ./bin/www",
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run dev -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "bcrypt": "5.0.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "events": "3.0.0",
    "express": "4.16.4",
    "express-fileupload": "1.1.6",
    "express-subdomain": "1.0.5",
    "http": "0.0.0",
    "http-errors": "1.6.3",
    "indicative": "7.2.3",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.9.1",
    "nodemailer": "6.3.1",
    "pg": "7.13.0",
